                    android:layout_weight=".25" />

                <Button
                    android:id="@+id/btnRefresh"
                    android:background="@drawable/refresh_icon" />
            </TableRow>

            <TableRow
                android:id="@+id/ErrorRowOnTravelListPage"
                android:paddingTop="5dp"
                android:visibility="invisible" >

